#include <iostream>

using namespace std;

void practice();

int main()
{
    practice();

    return 0;
}

void practice()
{
    for(int n=1; n<=10; n++)
    {
        cout<<'*';
    }
    cout<<endl;
    
    for(int n=1; n<=9; n++)
    {
        cout<<'*';
    }
    cout<<endl;
    
    for(int n=1; n<=8; n++)
    {
        cout<<'*';
    }
    cout<<endl;
    
    for(int n=1; n<=7; n++)
    {
        cout<<'*';
    }
    cout<<endl;
    
    for(int n=1; n<=6; n++)
    {
        cout<<'*';
    }
    cout<<endl;
    
    for(int n=1; n<=5; n++)
    {
        cout<<'*';
    }
    cout<<endl;
    
    for(int n=1; n<=4; n++)
    {
        cout<<'*';
    }
    cout<<endl;
    
    for(int n=1; n<=3; n++)
    {
        cout<<'*';
    }
    cout<<endl;
    
    for(int n=1; n<=2; n++)
    {
        cout<<'*';
    }
    cout<<endl;
    
    for(int n=1; n<=1; n++)
    {
        cout<<'*';
    }
}
